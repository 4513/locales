{
    "name": "mibo/locales",
    "type": "library",
    "description": "PHP Library for locale - Tags for Identifying Languages",
    "keywords": [
        "Tags for Identifying Languages",
        "Tags",
        "Language",
        "Languages",
        "Lang",
        "Locale",
        "Locales",
        "Country",
        "Countries",
        "RFC",
        "RFC 5646",
        "RFC5646",
        "IETF",
        "RFC 4646",
        "RFC4646",
        "RFC 3066",
        "RFC3066",
        "RFC 1766",
        "RFC1766",
        "PHP",
        "Codes",
        "Standard",
        "Language codes",
        "Internet Engineering Task Force",
        "MiBo",
        "Library",
        "Lib",
        "Package"
    ],
    "authors": [
        {
            "name": "Michal Boris",
            "email": "michal.boris27@gmail.com"
        }
    ],
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "MiBo\\Locales\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MiBo\\Locales\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^8.2",
        "mibo/languages": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.1",
        "phpunit/php-invoker": "^4.0",
        "phpstan/phpstan": "^1.5",
        "phpstan/phpstan-strict-rules": "^1.5",
        "jetbrains/phpstorm-attributes": "^1.0",
        "slevomat/coding-standard": "^8.13",
        "phpstan/extension-installer": "^1.3",
        "php-parallel-lint/php-parallel-lint": "^1.3"
    },
    "config": {
        "platform-check": true,
        "lock": false,
        "allow-plugins": {
            "phpstan/extension-installer": true,
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
